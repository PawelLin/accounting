page {
    height: 100%;
}
.annual {
    padding-top: 30px;
    display: flex;
    flex-direction: column;
    height: 100%;
    box-sizing: border-box;
    background: linear-gradient(180deg, #f6dbd3, #ede0ca);
}
.annual-top {
    margin-top: 0px;
    padding-left: 20px;
}
.annual-bar {
    margin-top: -50px;
}
.annual-bottom {
    margin-top: -80px;
    padding-right: 20px;
    text-align: right;
}
.annual-year {
    font-size: 24px;
    font-weight: bold;
}
.annual-label {
    margin-top: 12px;
    font-size: 16px;
}
.annual-value {
    color: #e35c2f;
    font-size: 22px;
    font-weight: bold;
}
.annual-unit {
    margin: 0 10px;
    font-size: 14px;
    color: #ada092;
    font-weight: normal;
}
.annual-total {
    margin-top: 20px;
    font-size: 24px;
}